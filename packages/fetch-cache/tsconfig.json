{
  "extends": "@fetch-cache/tsconfig/library-strict",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "./dist",
    "declarationDir": "./dist",
    "paths": {
      "fetch-cache": ["src"],
      "fetch-cache/config": ["src/lib/Config"],
      "fetch-cache/cache": ["src/lib/Cache"],
      "fetch-cache/redis": ["src/lib/Cache/redis"],
      "fetch-cache/utils": ["src/lib/Utils"]
    }
  },
  "include": ["src/**/*.ts", "eslint.config.mjs"]
}
