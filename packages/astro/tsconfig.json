{
  "extends": "@fetch-cache/tsconfig/library-strict",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "./dist",
    "declarationDir": "./dist",
    "noImplicitAny": false,
    "paths": {
      "@fetch-cache/astro": ["src"],
      "@fetch-cache/astro/*": ["src/*"]
    }
  },
  "include": ["src/types/*.d.ts", "src/**/*.ts", "eslint.config.mjs"]
}
